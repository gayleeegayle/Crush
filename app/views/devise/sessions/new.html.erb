<br>
<div class="row">
  <div class="col-md-5 center">
    <table class="table table-bordered">
      	<tr class="tableh">
        	<th>
          		<center><h1>Log in</h1></center>
        	</th>
      	</tr>
      	<tr>
      		<td class="tdspace">
				<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
					 <div class="form-inputs">
					    <%= f.input :email, required: false, autofocus: true %><br>
					    <%= f.input :password, required: false %><br>
					    <!-- <%= f.input :remember_me, as: :boolean if devise_mapping.rememberable? %><br> -->
					 </div>

					 <center>
					   	<%= f.button :submit, "Log in", class: "tdspace btn btn-success btn-sm raised" %><br><br>
					 </center>
				<% end %>
			</td>
		</tr>
	</table>

				<%= render "devise/shared/links" %>
